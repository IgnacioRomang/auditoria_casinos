select SUM(coinin) as total_apostado , SUM(coinout) as total_premios , SUM(jackpot) as total_pmayores , SUM(valor) as total_beneficio, ROUND(AVG(promedio_por_maquina),2) as total_promedio, ROUND(AVG(porcentaje_devolucion),2) as total_devolucion, casino.nombre as nombreCasino,tipo_moneda.descripcion as moneda
from beneficio JOIN casino on casino.id_casino= beneficio.id_casino join tipo_moneda on tipo_moneda.id_tipo_moneda = beneficio.id_tipo_moneda
where Year(fecha) =  ,Month(fecha) =  ,
group by(casino.nombre,moneda)


